            <div class="col-lg-12">
                <ul id="portfolio-flters">
                    <li data-filter="*" class="filter-active">All</li>
                    <li data-filter=".filter-app">Biệt Thự </li>
                    <li data-filter=".filter-card">Nhà Nguyên Căn</li>
                    <li data-filter=".filter-web">Cơ Khí</li>
                    <li data-filter=".filter-Nhaxuong">Nhà Xưởng</li>
                    <li data-filter=".filter-nhapho">Nhà Phố</li>
                </ul>
            </div>
        </div>
     
        @foreach (var item in Model.dsSanPham)
        {
            <div class="col-lg-4 col-md-6 portfolio-item filter-app">
                @*Danh Sách Biệt Thự *@
                <div class="portfolio-wrap">
                    <img src="@item.HinhAnh" class="img-fluid" alt="">
